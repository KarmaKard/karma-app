{
  "name": "karma-app-client",
  "version": "1.0.0",
  "private": true,
  "description": "The Karma React App",
  "main": "client/app.js",
  "scripts": {
    "start": "rm -rf dist && mkdir -p dist && cp -r assets/* dist && webpack-dev-server --port 4200",
    "winstart": "webpack-dev-server --port 4200",
    "build": "rm -rf dist && mkdir -p dist && cp -r assets/* dist && webpack -p",
    "test": "mocha --reporter min --compilers js:babel/register 'app/**/*.spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KarmaKard/karma-app.git"
  },
  "author": "tgroshon",
  "bugs": {
    "url": "https://github.com/KarmaKard/karma-app/issues"
  },
  "homepage": "https://github.com/KarmaKard/karma-app",
  "dependencies": {
    "babel": "5.1.10",
    "babel-core": "5.1.10",
    "babel-loader": "5.0.0",
    "babel-runtime": "5.1.10",
    "cheerio": "0.19.0",
    "config": "1.12.0",
    "cssnext-loader": "1.0.1",
    "dotenv": "~0.4.0",
    "envify": "~1.2.1",
    "envify-loader": "~0.1.0",
    "file-loader": "0.8.1",
    "glob": "~4.0.6",
    "img-loader": "1.0.2",
    "imports-loader": "~0.6.3",
    "lodash": "3.7.0",
    "mocha": "2.2.4",
    "path-to-url": "0.1.0",
    "react": "0.13.2",
