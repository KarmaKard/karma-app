    "babel-core": "5.1.10",
    "babel-loader": "5.0.0",
    "babel-runtime": "5.1.10",
    "config": "1.12.0",
    "cssnext-loader": "1.0.1",
    "dotenv": "~0.4.0",
    "envify": "~1.2.1",
    "envify-loader": "~0.1.0",
